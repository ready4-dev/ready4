Package: ready4
Title: Implement Modular Health Economic Models
Version: 0.0.0.9133
Authors@R: c(
    person("Matthew", "Hamilton", , "matthew.hamilton1@monash.edu", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0001-7407-9194")),
